		<ip>qinfengjishu.imwork.net</ip>
		<!--填服务器web端口80-->
		<port>80</port>
		<!--登录超时时间，可以是小数，单位为秒-->
		<waittime>3</waittime>
		<!--登录最大尝试次数-->
		<trytimes>3</trytimes>
	</server>
	<client>
		<username>cava</username>
		<password>12345678</password>
		<login_type>1</login_type>
		<!--CAVA_SPACE3的绝对路径，默认为D:\CAVA3\CAVA_SPACE3-->
		<cava_abs_pth>D:\CAVA3\CAVA_SPACE3</cava_abs_pth>
	</client>
	<mail_cfg>
		<!--邮件通知使能开关，0-关闭，1-开启-->
