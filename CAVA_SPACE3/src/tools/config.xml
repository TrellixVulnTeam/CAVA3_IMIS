		<!--发件服务器-->
		<mail_server>smtp.sina.cn</mail_server>
		<!--邮箱用户名，格式举例: xx@qq.com-->
		<username>yzbemail@sina.cn</username>
		<!--邮箱密码-->
		<password>xxxxxx</password>
		<!--发件人名称，同邮箱用户名-->
		<sender_name>yzbemail@sina.cn</sender_name>
		<!--邮件接收人，用;隔开-->
		<receivers>905406533@qq.com</receivers>
	</mail_cfg>
